{
    "sourceId": "fanabc",
    "url": "http://www.fanabc.com/index.php/news.html",
    "scope": ".catItemView",
    "selectors": {
        "title": ".catItemTitle a",
        "url": ".catItemTitle a@href",
        "description": ".catItemIntroText",
        "thumbnailUrl": ".catItemImage img@src"
    },
    "textSelector": ".itemFullText",
    "encodeURI": true,
    "paginate": ".pagination-next a@href",
    "limit": 3,
    "postType": "article"
}